{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "quotes": 0,
    "no-unused-vars": 0,
    "no-useless-constructor": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "no-console": 0
  }
}
